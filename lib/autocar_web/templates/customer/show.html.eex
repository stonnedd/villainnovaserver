<h2>Show Customer</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @customer.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @customer.email %>
  </li>

  <li>
    <strong>Phone:</strong>
    <%= @customer.phone %>
  </li>

  <li>
    <strong>Password:</strong>
    <%= @customer.password %>
  </li>

</ul>

<span><%= link "Edit", to: customer_path(@conn, :edit, @customer) %></span>
<span><%= link "Back", to: customer_path(@conn, :index) %></span>
