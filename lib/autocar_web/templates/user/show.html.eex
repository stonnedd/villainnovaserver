<h2>Show User</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Hash password:</strong>
    <%= @user.password %>
  </li>

  <li>
    <strong>Profile:</strong>
    <%= @user.profile %>
  </li>

  <li>
    <strong>Token:</strong>
    <%= @user.token %>
  </li>

</ul>

<span><%= link "Edit", to: user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: user_path(@conn, :index) %></span>
